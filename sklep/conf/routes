
GET     /                           controllers.ProductController.index
# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

### Address routing ###
GET     /address/add                controllers.AddressController.addAddress
POST    /address/addhandle          controllers.AddressController.addAddressHandle
GET     /address/update/:id         controllers.AddressController.updateAddress(id: Int)
POST    /address/updatehandle       controllers.AddressController.updateAddressHandle
GET     /address/delete/:id         controllers.AddressController.deleteAddress(id: Int)
GET     /address/all                controllers.AddressController.getAllAddresses
GET     /address/:id                controllers.AddressController.getAddress(id: Int)

# Address json routng #

GET     /address/all/json           controllers.AddressController.getAllAddressesJson
GET     /address/:id/json           controllers.AddressController.getAddressByIdJson(id: Int)
DELETE  /address/:id/json           controllers.AddressController.deleteAddressJson(id: Int)
POST    /address/add/json           controllers.AddressController.addAddressJson
PUT     /address/update/:id/json    controllers.AddressController.updateAddressJson(id: Int)

### Cart routing ###
GET     /cart/add                   controllers.CartController.addCart
POST    /cart/addhandle             controllers.CartController.addCartHandle
GET     /cart/update/:id            controllers.CartController.updateCart(id: Int)
POST    /cart/updatehandle          controllers.CartController.updateCartHandle
GET     /cart/delete/:id            controllers.CartController.deleteCart(id: Int)
GET     /cart/all                   controllers.CartController.getAllCarts
GET     /cart/:id                   controllers.CartController.getCart(id: Int)

# Cart json routng #

GET     /cart/all/json              controllers.CartController.getAllCartsJson
GET     /cart/:id/json              controllers.CartController.getCartByIdJson(id: Int)
DELETE  /cart/:id/json              controllers.CartController.deleteCartJson(id: Int)
POST    /cart/add/json              controllers.CartController.addCartJson
PUT     /cart/update/:id/json       controllers.CartController.updateCartJson(id: Int)

